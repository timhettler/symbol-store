{
  "name": "@timhettler/symbol-store",
  "description": "Combine SVGs into a single file with Symbol definitions",
  "version": "1.0.0",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "bin": "./dist/bin/symbol-store.js",
  "license": "MIT",
  "keywords": [
    "ui",
    "svg",
    "sprite",
    "symbol",
    "react",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/timhettler/symbol-store"
  },
  "scripts": {
    "build": "run-s build:code build:permissions",
    "build:code": "tsc",
    "build:permissions": "chmod +x ./dist/bin/*",
    "check-types": "tsc --noEmit",
    "lint": "run-s lint:code lint:format",
    "lint:code": "eslint . --fix",
    "lint:format": "prettier . --write",
    "test": "run-p test:types test:unit test:output",
    "test:types": "tsc --noEmit",
    "test:unit": "mocha",
    "test:output": "./dist/bin/symbol-store.js -i ./__test__/icons -o ./__test__/out"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/cheerio": "^0.22.35",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.6",
    "assert": "^2.1.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "globals": "^15.14.0",
    "mocha": "^11.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "dependencies": {
    "cheerio": "^1.0.0",
    "commander": "^13.0.0",
    "svgo": "^3.3.2"
  },
  "peerDependencies": {
    "react": "*"
  }
}
